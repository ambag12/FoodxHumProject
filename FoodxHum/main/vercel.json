{
    "builds": [
        {
            "src": "FoodxHum/main/main/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.11",
                "buildCommand": "bash build.sh",
		"maxLambdaSize": "15mb",
                "includeFiles": [
                    "FoodxHum/main/**",
                    "FoodxHum/main/main/**",
                    "FoodxHum/crt/**",
                    "requirements.txt"
                ]
            }
        },
        {
            "src": "FoodxHum/main/public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "FoodxHum/main/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "FoodxHum/main/main/wsgi.py"
        }
    ]
}