{
    "builds": [
        {
            "src": "FoodxHum/main/manage.py",
            "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.13" }
 
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "FoodxHum/main/manage.py"
        }
    ],
    "outputDirectory": "public"
}