{
    "builds": [
        {
            "src": "FoodxHum/main/main/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.13" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/(.*)",
            "dest": "FoodxHum/main/main/wsgi.py"
        }
    ],
    "outputDirectory": "FoodxHum/main/main/staticfiles"
}