{
    "builds": [
        {
            "src": "main/manage.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "main/manage.py"
        }
    ],
    "outputDirectory": "public"
}